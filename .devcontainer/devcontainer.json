{
  "name": "Cavoke",
  "image": "ghcr.io/cavoke-project/dev",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "ms-vscode.cmake-tools",
        "ms-azuretools.vscode-docker",
        "timonwong.shellcheck"
      ]
    }
  },
  "forwardPorts": [8080],
  "onCreateCommand": "docker build . -f db.Dockerfile -t cavoke_db"
}